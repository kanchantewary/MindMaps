        <mxCell id="NALcq5_tKgL-kjJS8C9A-1" value="Accelerated networking" style="rounded=1;whiteSpace=wrap;html=1;fontFamily=Tahoma;" parent="5B0C41dTFPrl-VRZk0_j-1" vertex="1">
